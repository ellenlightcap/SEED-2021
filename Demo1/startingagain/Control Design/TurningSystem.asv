data = importdata('TurnLeftNegative(128)(50msSample).txt');
phidot = data(:,12);
time = .05*(0:length(phidot)-1);
figure(1)
plot(time,phidot);
ylabel('in/sec')
xlabel('sec')
K=2.4/(2*128);
sigma = 10;
sys = tf(K*sigma,[1 sigma]);
[y,ts]=step(sys*(2*128));
hold on
plot(ts+1,y);
hold off

%
% PI controller
%
Kp=20;

C = tf(10*[2  20],[1 0]);
%
% step response of closed loop system
%
figure(2)
step(feedback(sys*C,1))